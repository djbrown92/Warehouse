@page
@model Warehouse.Pages.Stock.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Inventory[0].Compnum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inventory[0].Inventoryref)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inventory[0].Whousecode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inventory[0].Inventoryqty)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Inventory) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Compnum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inventoryref)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Whousecode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inventoryqty)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
